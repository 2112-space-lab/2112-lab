{
    "name": "app-service",
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "go build -o dist/org/app-service ./app-service"
            }
        },
        "serve": {
            "executor": "nx:run-commands",
            "options": {
                "command": "go run ./app-service/internal/main.go"
            }
        }
    }
}